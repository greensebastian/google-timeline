@model List<GoogleTimelineUI.Models.LocationCountModel>
@{
}
<table class="table table-striped table-responsive">
    <tr>
        <th>Place Id</th>
        <th>Name</th>
        <th>Count</th>
        <th>Address</th>
    </tr>    
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.LocationId</td>
            <td>@item.LocationName</td>
            <td>@item.VisitCount</td>
            <td><a href="@item.MapsLink" target="_blank">@item.Address</a></td>
        </tr>
    }
</table>